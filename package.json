{
  "publisher": "simonhe",
  "name": "vscode-css-selector",
  "displayName": "vscode-css-selector",
  "version": "0.0.5",
  "packageManager": "pnpm@8.15.9",
  "description": "vscode-css-selector",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/vscode-css-selector#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/vscode-css-selector"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/vscode-css-selector/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.cjs",
  "icon": "icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.92.0"
  },
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:html",
    "onLanguage:javascriptreact",
    "onLanguage:less",
    "onLanguage:postcss",
    "onLanguage:scss",
    "onLanguage:svelte",
    "onLanguage:typescriptreact",
    "onLanguage:vue"
  ],
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsdown src/index.ts --external vscode --format cjs",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.1",
    "@types/node": "^18.19.49",
    "@types/vscode": "^1.92.0",
    "@vscode-use/utils": "^0.1.61",
    "@vscode/vsce": "^3.7.1",
    "bumpp": "^9.1.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "vitest": "^4.0.16"
  }
}
